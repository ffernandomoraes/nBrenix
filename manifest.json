{
	"manifest_version": 2,
	"name": "nBrenix",
	"version": "1.3",
	"description": "Troque todas as imagens do site por fotos do Brenix.",
	"icons": {
    	"48": "icon48.png",
        "128": "icon128.png"
    },
	"content_scripts": [
	    {
	      "matches": ["*://*/*"],
	      "js": ["jquery.js", "nBrenix.js"]
	    }
  	]
}